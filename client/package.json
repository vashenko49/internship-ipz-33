{
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "src/**/*.scss"
      ],
      "extensions": "scss"
    },
    "eslint-dev": {
      "patterns": [
        "src/**/*.js"
      ],
      "extensions": "js"
    }
  },
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "aframe": "^1.0.3",
    "axios": "^0.19.2",
    "cloudinary-core": "^2.8.0",
    "lodash": "^4.17.15",
    "object-to-formdata": "^3.0.5",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-aframe-ar": "^1.18.0",
    "react-dom": "^16.12.0",
    "react-emoji": "^0.5.0",
    "react-facebook-login": "^4.1.1",
    "react-github-login": "^1.0.3",
    "react-google-login": "^5.1.0",
    "react-material-ui-form-validator": "^2.0.10",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.1",
    "react-scroll-to-bottom": "^1.3.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "node-sass": "^4.13.0",
    "npm-watch": "^0.6.0",
    "prettier": "^1.19.1",
    "standart": "^6.1.0",
    "stylelint": "^11.1.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^3.1.1"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "concurrently \"yarn run watch-sass-js\" \"yarn run start-js\"",
    "heroku-cli": "react-scripts build",
    "build": "yarn run eslint-build && yarn run stylelint-build && react-scripts build",
    "eject": "react-scripts eject",
    "eslint-dev": "eslint --fix src/**/*.js --cache --cache-location .eslintcache",
    "eslint-build": "eslint --fix src/**/*.js",
    "stylelint-dev": "stylelint --fix src/**/*.scss --cache --cache-location .stylelintcache",
    "stylelint-build": "stylelint --fix src/**/*.scss",
    "watch-sass-js": "npm-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn run build",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000"
}
